<?xml version="1.0" encoding="utf8"?>
<!DOCTYPE web-app PUBLIC
"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

  <servlet>
    <servlet-name>$servlet_class_name$</servlet-name>
    <servlet-class>$package$.$servlet_class_name$</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>$servlet_class_name$</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

  <listener>
        <listener-class>$package$.InitializationListener</listener-class>
  </listener>

</web-app>


